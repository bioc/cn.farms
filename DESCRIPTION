    'createMatrix.R' 'distributionDistance.R' 'dnaCopySf.R'
    'doCnFarms.R' 'fragLengthCorr.R' 'normAdd.R' 'normalizeAverage.R'
    'normalizeCels.R' 'normalizeNpData.R' 'normalizeQuantiles.R'
